<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping
        PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.codigonline.entities.Alumno" table="alumnos" schema="m06">
        <id name="id" column="idAlumno">
            <generator class="increment"/>
        </id>
        <property name="nombre"/>
        <property name="nacionalidad"/>
        <property name="sexo"/>
        <property name="edad"/>
        
    </class>
</hibernate-mapping>